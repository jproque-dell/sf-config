gearman-host: "{{ job_logs_gearman_client_hostname }}"
gearman-port: 4731
output-host: {{ external_logstash_host if external_logstash_host else logstash_host }}
output-port: {{ external_logstash_port if external_logstash_port else logstash_input_port }}
output-mode: tcp
#crm114-script: /usr/local/bin/classify-log.crm
#crm114-data: /var/lib/crm114
#log-ca-certs: /path/to/http_log_ca_cert
{% if 'sftests.com' in fqdn %}
log-cert-verify: False
{% else %}
log-cert-verify: True
{% endif %}
